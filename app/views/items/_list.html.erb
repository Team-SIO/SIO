<div class="form">
<%= form_tag search_items_path, method: :get do %>
  <%= text_field_tag :search, params[:search], class: "nav-box" %>
  <%= submit_tag '検索', class: "nav-box kensaku" %>
<% end %>
</div>

<div class="pro-edit">
  <% if user_signed_in? %>
	<%= link_to "プロフィール", user_path(current_user), class: "nav-box2" %>
  <% end %>

</div>