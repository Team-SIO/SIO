<%= form_for @order, url: admins_order_path, method: :patch do |a| %>
	<div class="form-group">
		<%= a.label :ユーザ名 %>
		<%= a.collection_select :user_id, User.all, :id, :name , :prompt => "選択してください",class: "form-control" %>
	</div>
	<div class="form-group">
		<%= a.label :送り先 %>
		<%= a.text_field :ship ,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= a.label :ステータス %>
		<%= a.select :status, Order.statuses.keys.to_a, {} %>
	</div>
	<%= a.submit "完了する", class: "btn btn-info mt-5" %>
<% end %>