<h1 class="mt-5">ジャンル一覧</h1>
<div class="mt-2">
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">ジャンル名</th>
      <th scope="col">編集</th>
    </tr>
  </thead>
   <% @genres.each do |genre|%>
  <tbody>
    <tr>
      <td><%= genre.try(:genre_name) %></td>
      <td><%=link_to "編集", edit_admins_genre_path(genre) ,style: "color: #000"%></td>
    </tr>

  </tbody>
  <% end %>
</table>
<div class="text-right">
	<%=link_to "戻る", admintop_path ,class: "back"%>
</div>
</div>

